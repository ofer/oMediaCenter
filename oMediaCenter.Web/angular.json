{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "oMediaCenter.Web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "wwwsource",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot",
            "index": "wwwsource/index.html",
            "main": "wwwsource/main.ts",
            "polyfills": "wwwsource/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "wwwsource/favicon.ico",
              "wwwsource/assets"
            ],
            "styles": [
              "wwwsource/custom-theme.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "wwwsource/styles.css"
            ],
            "scripts": [
              "node_modules/hls.js/dist/hls.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "wwwsource/environments/environment.ts",
                  "with": "wwwsource/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "oMediaCenter.Web:build:production"
            },
            "development": {
              "browserTarget": "oMediaCenter.Web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "oMediaCenter.Web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "wwwsource/test.ts",
            "polyfills": "wwwsource/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "wwwsource/favicon.ico",
              "wwwsource/assets"
            ],
            "styles": [
              "wwwsource/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "oMediaCenter.Web"
}
